{{- $.Scratch.Add "index" slice -}}
{{- range (where .Site.AllPages "Kind" "page") -}}
    {{- $.Scratch.Add "index" (dict "title" .Title "tags" .Params.tags "categories" .Params.categories "contents" .Plain "permalink" .RelPermalink "lang" .Language.Lang) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
